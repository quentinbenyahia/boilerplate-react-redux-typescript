{
  "name": "backoffice-react-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server -d",
    "build": "cross-env NODE_ENV=production npm run lint && webpack -p",
    "build-preprod": "cross-env NODE_ENV=development npm run lint && webpack -p",
    "lint": "./node_modules/.bin/eslint --ext .ts,.tsx src/",
    "lint-fix": "./node_modules/.bin/eslint --ext .ts,.tsx --fix src/; exit 0",
    "lint-fix-dry": "./node_modules/.bin/eslint --ext .ts,.tsx --fix-dry-run src/; exit 0",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o dist/storybook/",
    "clean": "rm -rf dist; rm -rf node_modules; npm install;",
    "deploy-prod": "npm run clean; npm run build; aws s3 sync dist/ s3://your-s3-storage --acl public-read --delete",
    "deploy-preprod": "rm -rf /dist; run build-preprod; npm run build-storybook; aws s3 sync dist/ s3://your-s3-storage --acl public-read --delete"
  },
